import java.util.*;
public class Main
{
	public static int main(String[] args) {
	    int i,j;
	    {
	        for(i=1; i<=4; i++)
	        {
	            for(j=4; j>=1; j--)
	            {
	                if(j==0)
	                    return 0;
	                System.out.println(i/j);
	            }
	        }
	    }
	    return 1;
	}
}
