public class Main   
{  
  public static void main(String args[])  
  {  
    int i, j, n, key = 0,tmp;  
    int a[] = {11,2,2,1,5};
    int b[] = {22,4,6,3,10};
    float c[] = new float[a.length];
    n = a.length;
    for(i=0; i<n; i++)
    {
        c[i] = a[i] % b[i];
    }
    for(i=0; i<n; i++)
    {
        System.out.println(c[i]);
    }
  }
}
