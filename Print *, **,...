import java.util.*;
public class Main
{

	public static void main(String[] args) {
	int i, j,limit;

    Scanner scan=new Scanner(System.in);
	limit=scan.nextInt();
	    for(i=1; i<=limit; i++)
	    {
	        for(j=i; j<=limit; j++)
	        {
              System.out.print(" ");	        

	        }
	        for(j=1; j<=i; j++)
	        {
              System.out.print("*");	        

	        }
	        System.out.println(" ");
	    }
	}

}
