import java.util.*;
public class Main
{
	public static void main(String[] args) {
    int a[]={1,2,3,4,5,6,2};
    int c,length=0,i,sum=0;
   c=a.length;
   for(i=0;i<c;i++){

       sum=sum+a[i];
   }
        System.out.println("Sum of array is:"+sum);
        System.out.println("Length of Array is:"+c);


	}
}
