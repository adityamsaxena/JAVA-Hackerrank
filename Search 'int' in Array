import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int a[]={1,2,3,4,5}, key=0, c=-1;
	    Scanner scan=new Scanner(System.in);
	    key=scan.nextInt();
	    for(int i=0; i<=4; i++)
	    {
	       if(key==a[i])
	       {
	           c=i;
	           break;
	       }
	    }
	    if(c==-1)
	    {
	         System.out.println("Not Found");
	    }else{
	         System.out.println("Position is:"+(c+1));
	          System.out.println("Index is:"+c);
	    }
	}
}
