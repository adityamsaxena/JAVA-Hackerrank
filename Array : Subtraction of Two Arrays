public class Main   
{  
  public static void main(String args[])  
  {  
    int i, j, n, key = 0,tmp;  
    int a[] = {11,2,31,4,5};
    int b[] = {10,1,5,2,3};
    int c[] = new int[a.length];
    n = a.length;
    for(i=0; i<n; i++)
    {
        c[i] = a[i] - b[i];
    }
    for(i=0; i<n; i++)
    {
        System.out.println(c[i]);
    }
  }
}
