public class Main   
{  
  public static void main(String args[])  
  {  
    int i, n, j;  
    int a[] = {1,6,6,6,6,6,6,6,6,6,7,63,3,3,3,3,3,6,1,2,77,7,2,7,555,44,33,2,3,4,5};
    n = a.length;

    j=0;
    for(i=n-1; i>=0; i--)
    {
        System.out.println(j+"  "+i); 
        a[j]=a[i];
        j++;
    }
    
            System.out.print(" "+a[0]);  
  }
}
   
