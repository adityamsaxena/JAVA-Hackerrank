import java.util.*;
public class Main
{
    
	public static void main(String[] args) {
	int i, j,limit;
    int sum=0;
    Scanner scan=new Scanner(System.in);
	limit=scan.nextInt();
	    for(i=1; i<=limit; i++)
	    {
	        for(j=1; j<=10; j++)
	        {
              System.out.println(i+"x"+j+":"+i*j);	        }
	        
	    }
	}
    
}
